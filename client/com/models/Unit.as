package com.models {		import com.lib.Model;	import com.models.Player;		public class Unit extends Model {				private var type:String;		private var attack:int;		private var defence:int;		private var movement:int;		private var indirectCombat:Boolean;		private var owner:Player;			public function Unit(t:String, a:int, d:int, m:int, i:Boolean)		{		  	type = t;		  	attack = a;		  	defence = d;		  	movement = m;			indirectCombat = i;					}				public function setOwner(o:Player) { owner = o; }	}	}