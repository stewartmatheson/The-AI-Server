package com.models {		import com.lib.Model;		public class Tile extends Model {				private var type:String;		private var height:int;		private var mapOrder:int;		private var xpos:int;		private var ypos:int;				public function Tile(t:String, h:int, o:int, x:int, y:int)		{ 			type = t; 			height = h; 			mapOrder = o;			xpos = x;			ypos = y;		}				public function getHeight():int 		{ 			if(type == "Water")				return 6;			else				return height;		}		public function getType():String { return type; }		public function getXPos():int { return xpos; }		public function getYPos():int { return ypos; }	}}