package com.models {		import com.lib.Model;	import com.geom.MapPoint;		public class Tile extends Model {				private var type:String;		private var height:int;		private var mapOrder:int;		private var mapPos:MapPoint;				public function Tile(t:String, h:int, o:int, mP:MapPoint)		{ 			type = t; 			height = h; 			mapOrder = o;			mapPos = mP;		}				public function getHeight():int 		{ 			if(type == "Water")				return 6;			else				return height;		}		public function getType():String { return type; }		public function getMapPoint():MapPoint { return mapPos; } 	}}