package com.factories {		import com.Config;	import com.views.*;	import com.controllers.WelcomeScreenController;	import flash.events.MouseEvent;		public class Menus {		public static function createMapSelectionMenu(c:WelcomeScreenController):MenuView		{			var mapConfig:Array = Config.getValue("maps") as Array;			var m:MenuView = new MenuView();			for(var i:int = 0; i < mapConfig.length; i++ )			{				var b:Button = new Button(mapConfig[i]["name"]);				b.addEventListener(MouseEvent.CLICK, c.newBattle);				m.push(b);			}			return m;		}	}}