package com.factories {		import com.states.Battle;	import flash.display.Stage;	import com.models.Map;	import com.views.MapView;		public class BattleState {				public static function create(mapData:Array, currentStage:Stage):Battle 		{			var m:Map = Maps.createModel(mapData);			var mapView:MapView = Maps.createView(m);						var b:Battle = new Battle(currentStage);			b.setMap(m);			b.addView(mapView);			return b;		}	}	}