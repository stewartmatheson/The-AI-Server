package com.factories {		import com.models.*;	import com.views.*;	import flash.display.Stage;		public class Maps {			// Public Methods:		public static function createModel(mapData:Array):Map		{			var m:Map = new Map(mapData["width"], mapData["height"]);						for each(var currentTile:Array in mapData["tiles"])				m.pushBackTile(new Tile(currentTile["type"], currentTile["height"], currentTile["map-order"]));		    return m;		}				public static function createView(m:Map):MapView		{			var currentMapView:MapView = new MapView(m);			var tileArray:Array = new Array();			for(var i:int = 0; i < m.getTiles().length; i++ )			{				switch(m.getTiles()[i].getType())				{					case "Grass" :						tileArray.push(new GrassTile());					break;										case "Rock" :						tileArray.push(new RockTile());					break;										default:						trace("default hit");				}						}			currentMapView.setTiles(tileArray);			return currentMapView;		}	}}