package com.factories {		import com.models.Map;	import com.views.*;	import flash.display.Stage;		public class Maps {			// Public Methods:		public static function create(mapData:Array):Map		{			var currentMapView:MapView = new MapView(mapData["width"], mapData["height"]);			var m:Map = new Map(currentMapView);						var t:Array = new Array();						for each(var currentTile:Array in mapData["tiles"])			{				switch(currentTile["type"])				{					case "Grass" :						t.push(new GrassTile());					break;										default:						trace("default hit");				}			}		    currentMapView.setTiles(t);		    return m;		}	}}