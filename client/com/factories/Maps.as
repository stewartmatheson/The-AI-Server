package com.factories {		import com.models.*;	import com.views.*;	import flash.display.Stage;		public class Maps {			// Public Methods:		public static function createModel(mapData:Array):Map		{			var m:Map = new Map(mapData["width"], mapData["height"]);			var i:int = 0;			var cols:int = 0;			var rows:int = 0;						for each(var currentTile:Array in mapData["tiles"])			{				m.pushBackTile(new Tile(currentTile["type"], currentTile["height"], currentTile["map-order"], cols, rows));				i++;								cols++;				if(cols >= m.getWidth())				{					rows++;					cols = 0;				}			}			return m;		}				public static function createView(m:Map):MapView		{			var currentMapView:MapView = new MapView(m);			var tileArray:Array = new Array();						for(var i:int = 0; i < m.getTiles().length; i++)			{				switch(m.getTiles()[i].getType())				{					case "Grass" :						var gT:GrassTile = new GrassTile();						gT.setModel(m.getTiles()[i]);						tileArray.push(gT);					break;										case "Rock" :						var rT:RockTile = new RockTile();						rT.setModel(m.getTiles()[i]);						tileArray.push(rT);					break;										case "Water" :						var wT:WaterTile = new WaterTile();						wT.setModel(m.getTiles()[i]);						tileArray.push(wT);					break;				}			}			currentMapView.setTiles(tileArray);			return currentMapView;		}	}}