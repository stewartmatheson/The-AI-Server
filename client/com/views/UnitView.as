package com.views {		import flash.display.MovieClip;	import com.models.Unit;	import flash.events.Event;	import com.views.MapView;	import com.views.TileView;		public class UnitView extends MovieClip {				protected var model:Unit;		protected var mapV:MapView;				public function getModel():Unit { return model; }		public function setModel(u:Unit):void { model = u; }		public function setMapView(m:MapView):void { mapV = m; }		public function moved(e:Event):void		{			var targetTile:TileView = mapV.getTileAtPoint(model.getMapPoint());			x = targetTile.x + mapV.x;			y = targetTile.y + mapV.y;		}	}	}