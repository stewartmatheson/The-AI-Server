package com.views {		import flash.display.*;		public class MenuView extends MovieClip {				// Constants:		// Public Properties:		// Private Properties:		private var items:Array;			// Initialization:		public function MenuView() { items = new Array(); }				public function push(i:DisplayObject)		{ 			addChild(i);			items.push(i);		}				public function verticalLayout()		{			for(var i:int = 0; i < items.length; i++)			{				items[i].y = items[i].y + items[i].height * i + (20 * i); 			}		}			// Public Methods:		// Protected Methods:	}	}