package com.states {		import com.lib.State;	import flash.display.Stage;	import com.Config;	import com.views.Button;		public class WelcomeScreen extends State {				public function WelcomeScreen(s:Stage) 		{			super(s);			var mapConfig:Array = Config.getValue("maps") as Array;						for(var i:int = 0; i < mapConfig.length; i++ )			{				stateViews.push(new Button(mapConfig[i]["name"]));			}			displayState();		}	}}