package com.lib {	import flash.display.Stage;	import flash.events.*;		public class State extends EventDispatcher {		protected var currentStage:Stage;		protected var stateViews:Array;				public function State(s:Stage)		{ 			currentStage = s;			stateViews = new Array();		}				protected function displayState():void		{			for(var i:int; i < stateViews.length; i++ )				currentStage.addChild(stateViews[i]);		}	}}